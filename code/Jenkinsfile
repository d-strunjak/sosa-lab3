pipeline {
    agent any
    stages {
        stage('stage 1') {
            steps {
                echo 'Hello world!'
                bat 'python --version'

            }
        }
        stage('Test') {
            steps {
                bat 'cd test\\code'
                bat 'python unit_test_A.py'
            }
        }
        stage('Clear') {
            steps {
                bat 'rmdir /s /q test'


            }
        }
        
    }
}