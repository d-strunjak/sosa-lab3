pipeline {
    agent any
    stages {
        stage('stage 1') {
            steps {
                echo 'Hello world!'
                bat 'python --version'
            }
        }
        stage('Clone') {
            steps {
                git url: 'https://github.com/d-strunjak/sosa-lab3.git'
                bat 'dir'
                bat 'git checkout main'
            }
        }
    }
}